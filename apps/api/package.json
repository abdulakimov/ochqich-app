{
  "name": "@ochqich/api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "npm run build && node --test dist/**/*.test.js",
    "build": "npm run build:shared && tsc -p tsconfig.json",
    "build:shared": "npm --prefix ../../packages/shared run build",
    "start": "node dist/index.js",
    "prisma:generate": "node scripts/prisma-with-env.cjs generate",
    "prisma:migrate": "node scripts/prisma-with-env.cjs migrate dev",
    "prisma:deploy": "node scripts/prisma-with-env.cjs migrate deploy"
  },
  "dependencies": {
    "@ochqich/shared": "file:../../packages/shared",
    "@prisma/client": "^5.20.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.10",
    "prisma": "^5.20.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
